#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/zand/Code/Incubate/microservice/node_modules/.pnpm/unbuild@2.0.0_typescript@5.5.4/node_modules/unbuild/dist/node_modules:/Users/zand/Code/Incubate/microservice/node_modules/.pnpm/unbuild@2.0.0_typescript@5.5.4/node_modules/unbuild/node_modules:/Users/zand/Code/Incubate/microservice/node_modules/.pnpm/unbuild@2.0.0_typescript@5.5.4/node_modules:/Users/zand/Code/Incubate/microservice/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/zand/Code/Incubate/microservice/node_modules/.pnpm/unbuild@2.0.0_typescript@5.5.4/node_modules/unbuild/dist/node_modules:/Users/zand/Code/Incubate/microservice/node_modules/.pnpm/unbuild@2.0.0_typescript@5.5.4/node_modules/unbuild/node_modules:/Users/zand/Code/Incubate/microservice/node_modules/.pnpm/unbuild@2.0.0_typescript@5.5.4/node_modules:/Users/zand/Code/Incubate/microservice/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../unbuild/dist/cli.mjs" "$@"
else
  exec node  "$basedir/../unbuild/dist/cli.mjs" "$@"
fi
