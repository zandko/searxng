#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/zand/Code/Incubate/microservice/node_modules/.pnpm/pnpm@9.6.0/node_modules/pnpm/bin/node_modules:/Users/zand/Code/Incubate/microservice/node_modules/.pnpm/pnpm@9.6.0/node_modules/pnpm/node_modules:/Users/zand/Code/Incubate/microservice/node_modules/.pnpm/pnpm@9.6.0/node_modules:/Users/zand/Code/Incubate/microservice/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/zand/Code/Incubate/microservice/node_modules/.pnpm/pnpm@9.6.0/node_modules/pnpm/bin/node_modules:/Users/zand/Code/Incubate/microservice/node_modules/.pnpm/pnpm@9.6.0/node_modules/pnpm/node_modules:/Users/zand/Code/Incubate/microservice/node_modules/.pnpm/pnpm@9.6.0/node_modules:/Users/zand/Code/Incubate/microservice/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/pnpm@9.6.0/node_modules/pnpm/bin/pnpm.cjs" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/pnpm@9.6.0/node_modules/pnpm/bin/pnpm.cjs" "$@"
fi
